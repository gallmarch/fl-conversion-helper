{
  "manifest_version": 2,
  "name": "Fallen London Conversion Helper",
  "description": "This extension adds a category of side-convertible items to your inventory.",
  "permissions": [
    "storage"
  ],
  "version": VERSION,
  "content_scripts": [
    {
      "matches": ["http://fallenlondon.storynexus.com/Gap/Load*"],
      "css": [
        "styles.css"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
